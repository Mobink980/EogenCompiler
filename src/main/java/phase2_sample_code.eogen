class Program {

  function run (a, b) of instance {
    if (a > b) {
      for (counter = 0; counter < a; counter = counter + 1) {
        exec print(text : exec NewLineString(value: "Hello world"))
      }
    }
    else {
      exec print(text : exec NewLineString(value: "Bye"))
    }
  }
}

num1 = 10
num2 = 5

p = exec Program()

exec p.run(a : num1, b : num2)